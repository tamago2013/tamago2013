      size = 1364, 689
    [[[child1]]]
      position = 344
      type = VPaned
      order = 0
      parent = child0
    [[[child2]]]
      position = 682
      type = HPaned
      order = 0
      parent = child1
    [[[child5]]]
      position = 452
      type = HPaned
      order = 1
      parent = child1
    [[[child7]]]
      position = 455
      type = HPaned
      order = 1
      parent = child5
    [[[terminal3]]]
      profile = default
      type = Terminal
      order = 0
      parent = child2
      title = ssm-coordinator
      command = ssm-coordinator; echo "-----"; echo "ssm-coordinator"; echo "-----"; bash; 
    [[[terminal4]]]
      profile = default
      type = Terminal
      order = 1
      parent = child2
     title = ssm-monitor
      command = sleep 0.5; ssm-monitor; echo "-----"; echo "ssm-monitor"; echo "-----"; bash; 
    [[[terminal6]]]
      profile = default
      type = Terminal
      order = 0
      parent = child5
     title = ypspur-coordinator
      command = sleep 1; ypspur-coordinator -p ${tamago_home}/data/Loop-new.param -d /dev/ttyACM0 --high-resolution; echo "-----"; echo "ypspur-coordinator -p "${tamago_home}"/data/Loop-new.param -d /dev/ttyACM0 --high-resolution"; echo "-----"; bash;  
    [[[terminal9]]]
      profile = default
      type = Terminal
      order = 1
      parent = child7
      title = ./working
      command = sleep 1; cd ${tamago_home}; echo "working directory:"; ./working; bash; 
    [[[terminal8]]]
      profile = default
      type = Terminal
      order = 0
      parent = child7
      title = distance-checker
      command = sleep 1; cd ${tamago_home}/distance-checker; ./launcher; echo "-----"; echo "./launcher"; echo "-----"; bash; 

