    [[[child1]]]
      type = Terminal
      parent = window0
      title = joystick
      command = ypspur-joystick -d /dev/input/js0 -V 1.0 -W 3.0 -A 1.0 -O 3.0 0.5 0.8; echo "-----"; echo "ypspur-joystick -d /dev/input/js0 -V 1.0 -W 3.0 -A 1.0 -O 3.0 0.5 0.8"; echo "-----"; bash;
    [[[window0]]]
      type = Window
      parent = ""

